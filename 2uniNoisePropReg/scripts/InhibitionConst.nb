Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"changesByRXN", "=", 
    RowBox[{"Function", "/@", 
     RowBox[{"<|", 
      RowBox[{
       RowBox[{"1", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"#1", "+", "1"}], ",", "#2", ",", "#3", ",", "#4"}], 
         "}"}]}], ",", 
       RowBox[{"2", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"#1", ",", 
          RowBox[{"#2", "+", "1"}], ",", "#3", ",", "#4"}], "}"}]}], ",", 
       RowBox[{"3", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"#1", "-", "1"}], ",", "#2", ",", "#3", ",", "#4"}], 
         "}"}]}], ",", 
       RowBox[{"4", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"#1", ",", 
          RowBox[{"#2", "-", "1"}], ",", "#3", ",", "#4"}], "}"}]}], ",", 
       RowBox[{"5", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"#1", ",", "#2", ",", 
          RowBox[{"#3", "+", "1"}], ",", "#4"}], "}"}]}], ",", 
       RowBox[{"6", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"#1", ",", "#2", ",", "#3", ",", 
          RowBox[{"#4", "+", "1"}]}], "}"}]}], ",", 
       RowBox[{"7", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"#1", ",", "#2", ",", 
          RowBox[{"#3", "-", "1"}], ",", "#4"}], "}"}]}], ",", 
       RowBox[{"8", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"#1", ",", "#2", ",", "#3", ",", 
          RowBox[{"#4", "-", "1"}]}], "}"}]}]}], "\[IndentingNewLine]", 
      "|>"}]}]}], ";", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"Each", " ", "hash", " ", "represents", " ", "a", " ", 
       RowBox[{"specie", ":", " ", 
        RowBox[{
        "mRNAA", "-", "proteinA", "-", "mRNAB", "-", "proteinB", "-", 
         "da0A"}], ":", 
        RowBox[{"promoterActivatedA", "-", 
         RowBox[{"da1A", ":", 
          RowBox[{"promoterInactivatedA", "-", "db0B", "-", 
           RowBox[{
           "db1B", "\n", "   ", "The", " ", "numbers", " ", "are", " ", "the",
             " ", "next", " ", "reactions"}]}], ":", "\[IndentingNewLine]", 
          RowBox[{"1", "-", "2", "-", "5", "-", "6"}]}]}]}]}], "\[Rule]", " ",
       "Synthesis"}], ",", "  ", 
     RowBox[{
      RowBox[{"3", "-", "4", "-", "7", "-", "8"}], "\[Rule]", " ", 
      "Degradation"}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{"pF", "=", 
    RowBox[{"Compile", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"h", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"kAB", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"kmA", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Gamma]mA", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"kpA", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Gamma]pA", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"kmB", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Gamma]mB", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"kpB", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Gamma]pB", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"mRNAA", ",", "_Integer"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"proteinA", ",", "_Integer"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"mRNAB", ",", "_Integer"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"proteinB", ",", "_Integer"}], "}"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"kmA", "+", "0.01"}], ",", 
        RowBox[{"kpA", "*", "mRNAA"}], ",", 
        RowBox[{"mRNAA", "*", "\[Gamma]mA"}], ",", 
        RowBox[{"\[Gamma]pA", "*", "proteinA"}], ",", 
        RowBox[{
         RowBox[{"kmB", "*", 
          FractionBox["1", 
           RowBox[{"1", "+", 
            SuperscriptBox[
             RowBox[{"(", 
              FractionBox["proteinA", "kAB"], ")"}], "h"]}]]}], "+", "0.01"}],
         ",", 
        RowBox[{"kpB", "*", "mRNAB"}], ",", 
        RowBox[{"mRNAB", "*", "\[Gamma]mB"}], ",", 
        RowBox[{"\[Gamma]pB", "*", "proteinB"}]}], "}"}], ",", 
      RowBox[{"RuntimeAttributes", "->", 
       RowBox[{"{", "Listable", "}"}]}], ",", 
      RowBox[{"Parallelization", "->", "True"}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}]], "Input",
