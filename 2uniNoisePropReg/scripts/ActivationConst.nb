 RowBox[{
  RowBox[{
   RowBox[{"changesByRXN", "=", 
    RowBox[{"Function", "/@", 
     RowBox[{"<|", 
      RowBox[{
       RowBox[{"1", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"#1", "+", "1"}], ",", "#2", ",", "#3", ",", "#4"}], 
         "}"}]}], ",", 
       RowBox[{"2", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"#1", ",", 
          RowBox[{"#2", "+", "1"}], ",", "#3", ",", "#4"}], "}"}]}], ",", 
       RowBox[{"3", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"#1", "-", "1"}], ",", "#2", ",", "#3", ",", "#4"}], 
         "}"}]}], ",", 
       RowBox[{"4", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"#1", ",", 
          RowBox[{"#2", "-", "1"}], ",", "#3", ",", "#4"}], "}"}]}], ",", 
       RowBox[{"5", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"#1", ",", "#2", ",", 
          RowBox[{"#3", "+", "1"}], ",", "#4"}], "}"}]}], ",", 
       RowBox[{"6", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"#1", ",", "#2", ",", "#3", ",", 
          RowBox[{"#4", "+", "1"}]}], "}"}]}], ",", 
       RowBox[{"7", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"#1", ",", "#2", ",", 
          RowBox[{"#3", "-", "1"}], ",", "#4"}], "}"}]}], ",", 
       RowBox[{"8", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"#1", ",", "#2", ",", "#3", ",", 
          RowBox[{"#4", "-", "1"}]}], "}"}]}]}], "\[IndentingNewLine]", 
      "|>"}]}]}], ";", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"Each", " ", "hash", " ", "represents", " ", "a", " ", 
       RowBox[{"specie", ":", " ", 
        RowBox[{
        "mRNAA", "-", "proteinA", "-", "mRNAB", "-", "proteinB", "-", 
         "da0A"}], ":", 
        RowBox[{"promoterActivatedA", "-", 
         RowBox[{"da1A", ":", 
          RowBox[{"promoterInactivatedA", "-", "db0B", "-", 
           RowBox[{
           "db1B", "\n", "   ", "The", " ", "numbers", " ", "are", " ", "the",
             " ", "next", " ", "reactions"}]}], ":", " ", 
          "\[IndentingNewLine]", 
          RowBox[{"1", "-", "2", "-", "5", "-", "6"}]}]}]}]}], "\[Rule]", " ",
