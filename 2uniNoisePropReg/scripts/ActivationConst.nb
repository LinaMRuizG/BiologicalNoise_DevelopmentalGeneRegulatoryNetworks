         RowBox[{"kmA", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Gamma]mA", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"kpA", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Gamma]pA", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"kmB", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Gamma]mB", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"kpB", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Gamma]pB", ",", "_Real"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"mRNAA", ",", "_Integer"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"proteinA", ",", "_Integer"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"mRNAB", ",", "_Integer"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"proteinB", ",", "_Integer"}], "}"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"kmA", "+", "0.01"}], ",", 
        RowBox[{"kpA", "*", "mRNAA"}], ",", 
        RowBox[{"mRNAA", "*", "\[Gamma]mA"}], ",", 
        RowBox[{"\[Gamma]pA", "*", "proteinA"}], ",", 
        RowBox[{
         RowBox[{"kmB", "*", 
          RowBox[{"(", 
           FractionBox[
            SuperscriptBox["proteinA", "h"], 
            RowBox[{"kAB", "+", 
             SuperscriptBox[
              RowBox[{"proteinA", " "}], "h"]}]], ")"}]}], "+", "0.01"}], ",", 
        RowBox[{"kpB", "*", "mRNAB"}], ",", 
        RowBox[{"mRNAB", "*", "\[Gamma]mB"}], ",", 
        RowBox[{"\[Gamma]pB", "*", "proteinB"}]}], "}"}], ",", 
      RowBox[{"RuntimeAttributes", "->", 
       RowBox[{"{", "Listable", "}"}]}], ",", 
      RowBox[{"Parallelization", "->", "True"}]}], "]"}]}], ";"}], 
